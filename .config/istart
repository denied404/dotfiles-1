# vim: ft=ruby

i3: 1
$ marvex
wait_window: 1-1
sleep: 1

i3: 2
$ marvex
wait_window: 2-1
sleep: 1

i3: q
$ firefox
wait_window: firefox
sleep: 1

i3: s
$ google-chrome-stable
wait_window: Google Chrome
sleep: 1

i3: e
$ marvex -e mutt
wait_window: e-1
sleep: 1

i3: z
$ marvex -e mcb n
$ marvex -e mcb p
wait_window: z-1
wait_window: z-2

$ notify-send "end"
