super + Return
	urxvt

super + r
	modal-run

super + shift + r
	modal-run --terminal

shift + Delete
	kill-focus

super + control + s
	browser

super + control + a
	brightness down

super + control + q
	brightness up

super + b
	xsel -op | xsel -ib

super + F1
	screenshot > ~/.screenshot

control + F9
	bluetooth-connect

control + F10
	sudo systemctl stop bluetooth

super + 8
	volume down

super + 9
	volume up

super + F5
	mplayer-control pt_step -1

super + F6
	mplayer-control pause

super + F7
	mplayer-control pt_step 1

control + Return
	screen-lock

alt + control + d
	notification-information-basic

super + control + e
	translator

super + Tab
	paster

control +  F11
	xrandr -s 0

control + F12
	environment-setup

super + grave
	kuake --terminal --bottom --middle --height 30 --width 50 --opacity 0.8

super + backslash
	kuake --focus --top --height 75 --width 100 --opacity 0.9

control + backslash
	autocomplete-tmux
