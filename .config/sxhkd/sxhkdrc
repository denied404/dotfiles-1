super + Return
	marvex -s -c

super + r
	modal-run

super + shift + r
	modal-run --terminal

shift + Delete
	kill-focus

super + control + s
	browser

super + control + a
	brightness down

super + control + q
	brightness up

super + b
	xsel -op | xsel -ib

super + F1
	screenshot > ~/.screenshot

control + F9
	bluetooth-connect

control + F10
	sudo systemctl stop bluetooth

super + 8
	volume down

super + 9
	volume up

super + F5
	mplayer-control pt_step -1

super + F6
	mplayer-control pause

super + F7
	mplayer-control pt_step 1

control + Return
	screen-lock

alt + control + d
	notification-information-basic

super + control + e
	translator

super + Tab
	paster

control + super +  F11
	autorandr --load laptop

control + super +  F12
	autorandr --load home

control + Delete
	environment-setup

super + grave
	kuake --terminal --bottom --middle --height 30 --width 50

super + backslash
	kuake --focus --bottom --height 75 --width 100

super + equal
	kuake --terminal --class todo --top --middle --height 30 --width 50 --exec "vim ~/.todo.md"

control + backslash
	autocomplete-tmux
