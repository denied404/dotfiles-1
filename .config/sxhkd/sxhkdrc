super + Return
	terminal

super + control + Return
	terminal-with-cwd

super + r
	modal-run -matching normal

super + g
	DB=~/.guts/.config/bookmarks.yaml bookmarks

control + super + g
	DB=~/.guts/.config/bookmarks.yaml bookmarks add

super + shift + r
	modal-run --terminal

shift + Delete
	kill-focus

super + control + s
	browser

super + control + w
	mrun spotify

super + control + f
	telegram-desktop

super + F1
	screenshot

super + F2
	screenshot deepin

control + F10
	brightness toggle

super + 8
	;volume down

super + 9
	;volume up

super + control + 9
	;pa-switch-sink && notify "$(pactl-get-default-sink)"

super + control + 0
	;pa-switch-profile -i webcam active output:hdmi-stereo output:analog-stereo

alt + control + d
	infobar

super + control + e
	translator

alt + Tab
	i3re -w 20 || i3re -h 20

super + Tab
	timer

super + 5
	carcosa-ui ~/.secrets:me ~/reconquest/secrets:re

control + super + 5
	carcosa-ui ~/.secrets:me ~/reconquest/secrets:re --mode login-password

super + 6
	carcosa-new-password

super + 7
	secrets_repository=~/.secrets carcosa-qr

super + grave
	;kuake-bottom

super + backslash
	i3gator-ui

super + equal
	;kuake --terminal --class top --top --middle --height 30 --width 50

control + F7
	wg-toggle

control + F8
	brightness down

control + F9
	brightness up

control + Pause
	screen-lock -suspend

control + super + Insert
	screen-lock -suspend

super + 0
	i3-msg 'gaps horizontal current toggle 400'

super + minus
	i3-msg 'gaps top current toggle 600'

control + F6
	bash -c 'x=$(xsel -o -c | darc); echo $x; xclip -selection clipboard <<< "$x"; notify "Uploaded to dard: $x"'
