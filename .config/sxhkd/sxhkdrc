super + Return
	marvex -s

super + r
	modal-run

super + shift + r
	modal-run --terminal

shift + Delete
	kill-focus

super + control + s
	browser

super + control + a
	brightness down

super + control + q
	brightness up

super + b
	xsel -op | xsel -ib

super + F1
	screenshot > ~/.screenshot

control + F9
	sudo systemctl stop bluetooth

control + F10
	sudo systemctl start bluetooth

super + 8
	volume down

super + 9
	volume up

super + F5
	mplayer-control pt_step -1

super + F6
	mplayer-control pause

super + F7
	mplayer-control pt_step 1

control + Return
	screen-lock

alt + control + d
	notification-information-basic

super + control + e
	translator

super + Tab
	paster

control + F12
	xkb

control + F11
	usb-automount

super + grave
	kuake --terminal --bottom --right --height 30 --width 50 --opacity 0.8

super + backslash
	kuake --focus --height 75

control + backslash
	autocomplete-tmux
