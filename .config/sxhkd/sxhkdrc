super + Return
	terminal

shift + super + Return
	terminal -e msh

control + Return
	terminal-with-cwd

super + r
	modal-run -matching normal

super + g
	DB=~/.guts/.config/bookmarks.yaml bookmarks

control + super + g
	DB=~/.guts/.config/bookmarks.yaml bookmarks add

super + shift + r
	modal-run --terminal

shift + Delete
	kill-focus

super + control + s
	browser

super + control + w
	mrun spotify

super + control + d
	upwork

super + control + f
	telegram-desktop

super + F1
	screenshot

super + F2
	screenshot deepin

control + F10
	brightness toggle

super + 8
	;volume down

super + 9
	;volume up

super + control + 9
	;pa-switch-sink && notify "$(pactl-get-default-sink)"

super + control + 0
	;pa-switch-profile -i webcam active output:hdmi-stereo output:analog-stereo

alt + control + d
	notification-information-basic

super + control + e
	translator

alt + Tab
	i3re -w 600

super + Tab
	timer

super + 5
	carcosa-ui ~/.secrets:me ~/reconquest/secrets:re

control + super + 5
	carcosa-ui ~/.secrets:me ~/reconquest/secrets:re --mode login-password

super + 6
	carcosa-new-password

super + 7
	secrets_repository=~/.secrets carcosa-qr

super + grave
	;kuake-bottom

super + backslash
	;kuake --focus --center --middle --height 90 --width 75

super + equal
	;kuake --terminal --class top --top --middle --height 30 --width 50

control + F5
	upwork-switch

control + F6
	VOLUME=30000 headset

control + F7
	openvpn-toggle

control + F8
	brightness down

control + F9
	brightness up

control + F11
	jidle-toggle
