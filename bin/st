#!/usr/bin/python2
# -*- coding: utf-8 -*-

import urllib2
import re

content = urllib2.urlopen("http://stat.rn/ngs/?module=sum_hours").read()
content = content.decode('utf-8')

matches = re.findall(ur'(?us)<td>([\w\s]+)<\/td>[^\d]+<b>(\d+)<\/b>', content)
for match in matches:
    (name,  hours) = match
    print u"{} {}".format(hours, name).encode("utf-8")
