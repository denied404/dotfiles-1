URxvt.iso14755: off
URxvt.iso14755_52: off

URxvt.perl-lib: /home/vxw/.urxvt/ext/perl

URxvt.buffered: false

URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

URxvt.perl-ext-common: autocomplete-ALL-the-things,clipboard,default,keyboard-select,vim-scrollback

URxvt.clipboard.copycmd:  xsel --input  --clipboard
URxvt.clipboard.pastecmd: xsel --output --clipboard

URxvt.keysym.Control-Shift-C: perl:clipboard:copy
URxvt.keysym.Control-Shift-V: perl:clipboard:paste
 
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-Shift-S: perl:keyboard-select:search

URxvt.keysym.M-C-slash:   perl:aAtt:WORD-complete
URxvt.keysym.M-question:  perl:aAtt:fuzzy-complete
URxvt.keysym.M-quotedbl:  perl:aAtt:undo

