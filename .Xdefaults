URxvt.iso14755: off
URxvt.iso14755_52: off

URxvt.perl-lib: /home/vxw/.urxvt/ext/perl

URxvt.buffered: false

URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

URxvt.perl-ext-common: autocomplete-ALL-the-things,clipboard,default,keyboard-select,url-select

URxvt.clipboard.copycmd:  xsel --input  --clipboard
URxvt.clipboard.pastecmd: xsel --output --clipboard

URxvt.keysym.Mod4-c: perl:clipboard:copy
URxvt.keysym.Mod4-v: perl:clipboard:paste
 
URxvt.keysym.Mod4-Escape: perl:keyboard-select:activate
URxvt.keysym.Mod4-Shift-S: perl:keyboard-select:search

URxvt.keysym.Mod4-C-slash:   perl:aAtt:WORD-complete
URxvt.keysym.Mod4-question:  perl:aAtt:fuzzy-complete
URxvt.keysym.Mod4-quotedbl:  perl:aAtt:undo

URxvt.keysym.Mod4-C-y: perl:url-select:select_next
URxvt.url-select.launcher: /home/vxw/bin/x-www-browser

!URxvt.matcher.pattern.0: https?\:\/\/git\.rn[\\w./?&@#-]*[\\w/-]
!URxvt.matcher.pattern.1: (\\bwww\\.|https?\:\/\/|ftp\:\/\/|file\:\/\/)[\\w-]+\\.[\\w./?&@#-]*[\\w/-]

