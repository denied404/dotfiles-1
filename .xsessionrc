#!/bin/sh

export PATH="$HOME/bin/:$HOME/go/bin/:$PATH"
export SHELL=/bin/zsh
export TERMINAL=urxvt
export EDITOR="/usr/bin/vim"

export BACKGROUND=$(cat $HOME/background)

xrandr --output VGA1 --mode 1920x1080 --pos 0x0 --output HDMI1 --mode 1920x1080 --pos 1920x0 &

$HOME/bin/reconfig-kbd &
sudo $HOME/bin/charge &
sudo $HOME/bin/acpi & # why dat shi doesn't running?
$HOME/bin/background $BACKGROUND &
feh --bg-scale ~/wallpaper.jpg &
