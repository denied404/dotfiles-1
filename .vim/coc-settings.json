{
    "python.jediEnabled": false,
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "diagnostic.errorSign": "\\ E",
    "diagnostic.warningSign": "\\ W",
    "diagnostic.infoSign": "\\ ∙",
    "java.home": "/usr/lib/jvm/java-8-openjdk/",
    "java.import.gradle.enabled": false,
    "xml.validation.schema": false,
    "xml.validation.noGrammar": "ignore",
    "javascript.suggestionActions.enabled": false,
    "git.topRemovedSign.text": "\\ ▔",
    "git.topRemovedSign.hlGroup": "GitDeleted",
    "git.removedSign.text": "\\ ▁",
    "git.removedSign.hlGroup": "GitDeleted",
    "git.addedSign.text": "\\ │",
    "git.addedSign.hlGroup": "GitAdded",
    "git.changedSign.text": "\\ │",
    "git.changedSign.hlGroup": "GitModified",
    "git.changeRemovedSign.text": "\\ ┊",
    "git.changeRemovedSign.hlGroup": "GitModified",
    "languageserver": {
        "terraform": {
            "command": "terraform-lsp",
            "filetypes": [
                "terraform"
            ],
            "initializationOptions": {}
        },
        "bash": {
            "command": "bash-language-server",
            "args": [
                "start"
            ],
            "filetypes": [
                "sh",
                "bash"
            ],
            "ignoredRootPaths": [
                "~"
            ]
        },
        "ccls": {
            "command": "ccls",
            "filetypes": [
                "c",
                "cpp",
                "objc",
                "objcpp"
            ],
            "rootPatterns": [
                ".ccls"
            ],
            "initializationOptions": {
                "cache": {
                    "directory": "/tmp/ccls"
                }
            }
        }
    },
    "java.trace.server": "verbose",
    "java.configuration.updateBuildConfiguration": "disabled",
    "java.errors.incompleteClasspath.severity": "ignore",
    "java.import.callback": "px#java#import#callback",
    "xml.format.splitAttributes": true,
    "coc.preferences.formatOnSaveFiletypes": [
        "xml"
    ],
    "pairs.enableCharacters": [
        "(",
        "[",
        "{",
        "'",
        "\"",
        "`"
    ],
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
    "coc.preferences.hoverTarget": "preview"
}
