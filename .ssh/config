IdentityFile ~/.ssh/id_rsa

Host t home.local
    HostName        home.local
    User            operator

Host aur.archlinux.org
    IdentityFile    ~/.ssh/aur/id_rsa_aur

Host deadcrew.org
    User root

Host git*
    ControlMaster   no
    StrictHostKeyChecking no

Host *
    ControlMaster   auto
    ControlPersist  1h
    ControlPath     ~/.ssh/connections/%r_%h_%p
    Compression     yes
    SendEnv         BACKGROUND
    User            e.kovetskiy
