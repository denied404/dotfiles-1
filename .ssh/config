IdentityFile ~/.ssh/id_rsa

Host aur.archlinux.org
    IdentityFile    ~/.ssh/aur/id_rsa_aur

Host deadcrew.org
    User root

Host github.com
    ControlMaster   yes

Host gitlab.com
    ControlMaster   yes

Host *.dead.archi
    ControlMaster   yes

Host desktop
    User operator
    ControlMaster   yes

Host desk
    User operator
    ControlMaster   yes

Host venus
    User operator
    ControlMaster   yes

Host *
    ControlMaster   no
    ControlPersist  1h
    ControlPath     /tmp/%r_%h_%p
    Compression     yes
    SendEnv         BACKGROUND
    User            root
    SendEnv         TERM=xterm
    ServerAliveInterval 100
